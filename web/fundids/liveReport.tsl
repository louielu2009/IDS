function _convertData(Data);
begin
     for i := 0 to length(Data)-1 do
     begin
          Data[i]['type'] := Data[i]['TYPE'];
          case Data[i]['type'] of
              'title':Data[i]['content'] := Data[i]['SRC2'];
              'txt':Data[i]['content'] := Data[i]['Data'] ? stm(Data[i]['Data']):'';
              'table','pic':Data[i]['content'] := Data[i]['Data'] ? stm(Data[i]['Data']):nil;
              'wordfile':Data[i]['content'] := '';
          end;
     end;
     return data;  
end;

